{
  "extends": [
    "next/core-web-vitals"
  ],
  "rules": {
    "no-unused-vars": "warn",
    "prefer-const": "error",
    "no-var": "error",
    "no-console": [
      "warn",
      {
        "allow": ["warn", "error"]
      }
    ],
    "eqeqeq": ["error", "always"],
    "curly": "off",
    "import/order": "off",
    "jsx-a11y/anchor-is-valid": "warn",
    "@next/next/no-img-element": "warn"
  },
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  }
}